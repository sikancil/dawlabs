{
  "name": "@dawlabs/shared-types",
  "version": "0.0.1",
  "description": "Shared TypeScript types for DAWLabs packages",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --target node18 --format esm,cjs --dts",
    "dev": "tsup src/index.ts --target node18 --format esm,cjs --dts --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "dawlabs",
    "types",
    "typescript",
    "shared"
  ],
  "author": "Arif Widianto <https://github.com/sikancil>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^22.15.3",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "tsup": "^8.5.1",
    "typescript": "5.9.2"
  }
}
