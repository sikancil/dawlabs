    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "commander": "^14.0.2",
    "undici": "^7.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "tsup": "^8.5.1"
  }
}
