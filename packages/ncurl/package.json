{
  "name": "@dawlabs/ncurl",
  "version": "1.0.0",
  "description": "A curl clone optimized for LLM cognitive patterns with intelligent HTTP inference",
  "main": "dist/index.js",
  "bin": {
    "ncurl": "dist/cli.cjs"
  },
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf dist",
    "start": "node dist/cli.cjs"
  },
  "keywords": [
    "dawlabs",
    "cli",
    "cli-tool"
  ],
  "author": "arif@dawlabs.dev",
  "license": "MIT",
