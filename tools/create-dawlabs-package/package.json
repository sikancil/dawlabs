{
  "name": "@dawlabs/create-package",
  "version": "0.0.2",
  "description": "CLI tool to create DAWLabs packages",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "create-dawlabs-package": "./dist/index.js",
    "dawlabs": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup index.js --target node18 --format esm --out-dir dist",
    "dev": "node index.js",
    "check-types": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^22.15.3",
    "eslint": "^9.39.1",
    "tsup": "^8.5.1",
    "typescript": "5.9.2"
  },
  "keywords": [
    "dawlabs",
    "cli",
    "generator",
    "template"
  ],
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "author": "Arif Widianto <https://github.com/sikancil>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
