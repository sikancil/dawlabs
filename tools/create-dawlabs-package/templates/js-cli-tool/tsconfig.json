{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "DAWLabs CLI Tool JavaScript",
  "extends": "../../config/tsconfig.cli.json",
  "compilerOptions": {
    // JavaScript Specific
    "allowJs": true,
    "checkJs": false,
    "declaration": false,
    "declarationMap": false,
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": [
    "tsup.config.ts"
  ]
}